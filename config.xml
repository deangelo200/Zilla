<?xml version='1.0' encoding='utf-8'?>
<widget id="com.phonegap.helloworld" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Factzilla</name>
    <description>
        A basic Framework7 template for PhoneGap.
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    
    <platform name="android">
    <icon src="icons/android/icon-36-ldpi.png"   platform="android" density="ldpi" />
    <icon src="icons/android/icon-48-mdpi.png"   platform="android"  density="mdpi" />
    <icon src="icons/android/icon-72-hdpi.png"   platform="android" density="hdpi" />
    <icon src="icons/android/icon-96-xhdpi.png"  platform="android"  density="xhdpi" />
    <icon src="icons/android/icon-144-xxhdpi.png"  platform="android"  density="xxhdpi" />
    <icon src="icons/android/icon-192-xxxhdpi.png"  platform="android" density="xxxhdpi" /> 
    </platform>
    
    
    <platform name="android">
        <preference name="android-minSdkVersion" value="14" />
        <allow-intent href="market:*" />
        <feature name="SocialSharing">
          <param name="android-package" value="nl.xservices.plugins.SocialSharing" />
        </feature>
    </platform>
    
    
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <preference name="BackupWebStorage" value="none" />
    </platform>
    
    <!-- Define app icon for each platform. -->
    
    

    
    <preference name="DisallowOverscroll" value="true" />
    <plugin name="cordova-plugin-whitelist" spec="~1.2.0" />
    <plugin name="cordova-plugin-console" spec="~1.0.1" />
    <plugin name="cordova-plugin-statusbar" spec="~1.0.1" />
    <plugin name="cordova-plugin-x-socialsharing" />
    <plugin name="com.darktalker.cordova.screenshot" />
    <plugin name="cordova-plugin-tts" />
    <plugin name="com.rjfun.cordova.mobfox" />
    
    
</widget>
